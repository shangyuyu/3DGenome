fid = fopen('test', 'w');

fprintf(fid, 'chr0\n');
fprintf(fid, '500\n');
fprintf(fid, '0\n');

x = 0.0;
y = 0.0;
z = 0.0;

for i = 1:200

    fprintf(fid, '%d %f %f %f ', i, x, y, z);
    x = x + 0.5/200.0;
end

for i = 201:400

    x = 0.5-
    fprintf(fid, '%d %f %f %f ', i, x, y, z);
end

fprintf('\n')
